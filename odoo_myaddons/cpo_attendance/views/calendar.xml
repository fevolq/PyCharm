<odoo>
    <data>
        <record id="attendance_calendar" model="ir.ui.view">
        <field name="model">cpo.cpo_attendance</field>
        <field name="name">attendance calendar</field>
        <field name="arch" type="xml">
            <calendar date_start="cpo_day" color="cpo_id" display="姓名：[cpo_name]，   工时：[cpo_times]">
                <field name="cpo_name"/>
                <field name="cpo_times"/>
            </calendar>
        </field>
        </record>
    </data>
</odoo>